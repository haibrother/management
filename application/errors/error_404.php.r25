<script type="text/javascript">
function countDown(secs,surl){
	document.getElementById("time_count_down").innerHTML = secs;
	if(--secs>0){
		setTimeout("countDown("+secs+",'"+surl+"')",1000);
	}   
	else{
		location.href=surl;
	}
}
